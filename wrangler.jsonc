{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "wda-2025-photo-comp",
	"compatibility_date": "2025-02-24",
	"compatibility_flags": ["nodejs_compat"],
	"main": "./workers/app.ts",
	"assets": {},
	"vars": {
		"FROM_EMAIL": "contact@messaging.portcityai.com",
		"FROM_NAME": "React Router Boilerplate",
		"SUPPORT_EMAIL": "support@example.com",
		"DEV_MODE": "true"
	},
	"observability": {
		"enabled": true
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "app-db",
			"database_id": "2060fb8d-100e-4ec5-8767-6992147757e5"
		}
	],
	"kv_namespaces": [
		{
			"binding": "SESSIONS",
			"id": "c2f40294d32e4a7f80fa41d36a9a8754"
		}
	],
	"r2_buckets": [
		{
			"binding": "PHOTO_STORAGE",
			"bucket_name": "photo-gallery-storage",
			"preview_bucket_name": "photo-gallery-storage-dev"
		}
	]
	/**
	 * Smart Placement
	 * Docs: https://developers.cloudflare.com/workers/configuration/smart-placement/#smart-placement
	 */
